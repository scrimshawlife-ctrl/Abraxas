{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Append A2 Terms Output",
  "description": "Output schema for memetic.registry.append capability",
  "type": "object",
  "required": ["appended", "registry_path", "source_run_id", "provenance", "not_computable"],
  "properties": {
    "appended": {
      "type": "integer",
      "description": "Number of terms appended to registry"
    },
    "registry_path": {
      "type": "string",
      "description": "Path to registry JSONL"
    },
    "source_run_id": {
      "type": "string",
      "description": "Source run ID used"
    },
    "provenance": {
      "type": "object",
      "description": "SHA-256 provenance envelope",
      "required": ["inputs_hash", "outputs_hash", "config_hash"],
      "properties": {
        "inputs_hash": {"type": "string"},
        "outputs_hash": {"type": "string"},
        "config_hash": {"type": "string"},
        "envelope_hash": {"type": "string"}
      }
    },
    "not_computable": {
      "type": "null",
      "description": "Always null for this capability"
    }
  },
  "additionalProperties": false
}
