{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Evidence Uplift Factors Output",
  "description": "Output schema for evidence.lift.uplift_factors capability",
  "type": "object",
  "required": ["attribution_uplift", "diversity_uplift", "provenance", "not_computable"],
  "properties": {
    "attribution_uplift": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 0.20,
      "description": "Bounded additive uplift for attribution_strength metric"
    },
    "diversity_uplift": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 0.15,
      "description": "Bounded additive uplift for source_diversity metric"
    },
    "provenance": {
      "type": ["object", "null"],
      "properties": {
        "operation_id": {
          "type": "string",
          "const": "evidence.lift.uplift_factors"
        },
        "inputs_sha256": {"type": "string", "pattern": "^[a-f0-9]{64}$"},
        "timestamp_utc": {"type": "string", "format": "date-time"}
      }
    },
    "not_computable": {
      "type": "null",
      "description": "Always null - operation never fails"
    }
  },
  "additionalProperties": false
}
