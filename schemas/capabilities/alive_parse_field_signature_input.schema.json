{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ALIVE Parse Field Signature Input",
  "description": "Input schema for alive.parse_field_signature capability",
  "type": "object",
  "required": ["field_signature"],
  "properties": {
    "field_signature": {
      "type": "object",
      "description": "Raw field signature dict to parse and validate"
    },
    "seed": {
      "type": ["integer", "null"],
      "description": "Deterministic seed for provenance"
    }
  },
  "additionalProperties": false
}
