{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Memetic Load Term Consensus Map Output",
  "description": "Output schema for memetic.term_consensus_map.load capability",
  "type": "object",
  "required": ["term_consensus_map", "provenance", "not_computable"],
  "properties": {
    "term_consensus_map": {
      "type": "object",
      "description": "Map of lowercase term â†’ consensus_gap value [0, 1]",
      "additionalProperties": {
        "type": "number",
        "minimum": 0.0,
        "maximum": 1.0
      }
    },
    "provenance": {
      "type": ["object", "null"],
      "properties": {
        "operation_id": {
          "type": "string",
          "const": "memetic.term_consensus_map.load"
        },
        "inputs_sha256": {"type": "string", "pattern": "^[a-f0-9]{64}$"},
        "timestamp_utc": {"type": "string", "format": "date-time"}
      }
    },
    "not_computable": {
      "type": "null",
      "description": "Always null - operation never fails (returns empty dict on error)"
    }
  },
  "additionalProperties": false
}
