{
  "version": "1.0.0",
  "description": "Daily Oracle v2 run configuration",
  "enabled_domains": [
    "politics",
    "media",
    "technology",
    "health"
  ],
  "sources": {
    "rss_feeds": [
      {"url": "https://rss.nytimes.com/services/xml/rss/nyt/Politics.xml", "domain": "politics"},
      {"url": "https://rss.nytimes.com/services/xml/rss/nyt/Technology.xml", "domain": "technology"},
      {"url": "https://rss.nytimes.com/services/xml/rss/nyt/Health.xml", "domain": "health"}
    ],
    "apis": [],
    "decodo": {
      "enabled": false,
      "query_templates": []
    }
  },
  "allow_sample_observations": false,
  "sample_observations": [],
  "dce": {
    "packs": []
  },
  "pipeline_config": {
    "enable_lifecycle_forecasting": true,
    "enable_resonance_detection": true,
    "enable_weather_trajectories": true,
    "confidence_threshold": 0.5
  },
  "output_config": {
    "save_to_ledger": true,
    "generate_intel": true,
    "generate_report": true,
    "retention_days": 90
  }
}
